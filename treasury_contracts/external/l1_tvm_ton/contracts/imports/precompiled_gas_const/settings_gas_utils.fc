#include "../stdlib.fc";

const int MIN_SETTINGS_STORAGE_DURATION = 10 * 365 * 24 * 3600; ;; 10 years

;;# Precompiled constants
;;
;;All of the contents are result of contract emulation tests
;;
;;The following numbers are obtained assuming empty cell as settings
const int SETTINGS_BITS = 29557; ;;[/tests/Settings.spec.ts#S-1.1]
const int SETTINGS_CELLS = 104; ;;[/tests/Settings.spec.ts#S-1.1]


int calculate_settings_min_storage_fee(cell settings) inline {
    int initial_gas = gas_consumed();
    (int settings_cells, int settings_bits, _) = compute_data_size(settings, 8192);
    int size_counting_gas = gas_consumed() - initial_gas;

    return get_compute_fee(BASECHAIN, size_counting_gas) + get_storage_fee(BASECHAIN, MIN_SETTINGS_STORAGE_DURATION, SETTINGS_BITS + settings_bits, SETTINGS_CELLS + settings_cells);
}
